{
    "expo": {
        "name": "goal-mobile",
        "slug": "goal-mobile",
        "version": "1.0.0",
        "orientation": "default",
        "icon": "./src/assets/icon.png",
        "userInterfaceStyle": "dark",
        "splash": {
            "image": "./src/assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "configChanges": ["orientation"],
            "adaptiveIcon": {
                "foregroundImage": "./src/assets/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            }
        },
        "web": {
            "favicon": "./assets/favicon.png"
        }
    }
}
